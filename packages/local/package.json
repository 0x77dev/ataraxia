{
  "name": "ataraxia-local",
  "version": "0.7.2",
  "description": "Machine-local transport for Ataraxia P2P messaging",
  "license": "MIT",
  "repository": "https://github.com/aholstenson/ataraxia/tree/master/packages/local",
  "main": "./dist/cjs/index.js",
