  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/jest": "^24.0.24",
    "@types/debug": "^4.1.5",
    "@types/msgpack-lite": "^0.1.7",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.3"
